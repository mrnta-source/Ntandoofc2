{
  "name": "mr-ntando-ofc",
  "version": "1.0.0",
  "description": "MR NTANDO OFC - Bot Expert Website with Interactive Features and Dead Bot Repair Services",
  "main": "js/index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "live-server --port=3000 --open=/main.html",
    "dev": "concurrently \"npm run serve\" \"npm run watch:css\"",
    "build": "npm run build:css && npm run build:js && npm run optimize",
    "build:css": "postcss css/style.css -o dist/css/style.min.css --use autoprefixer cssnano",
    "build:js": "terser js/main.js -o dist/js/main.min.js --compress --mangle",
    "watch:css": "postcss css/style.css -o css/style.min.css --use autoprefixer --watch",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "optimize": "npm run optimize:images && npm run optimize:html",
    "optimize:images": "imagemin images/* --out-dir=dist/images",
    "optimize:html": "html-minifier --input-dir . --output-dir dist --file-ext html --remove-comments --collapse-whitespace",
    "deploy": "npm run build && gh-pages -d dist",
    "clean": "rimraf dist",
    "validate": "html-validate *.html",
    "lighthouse": "lighthouse http://localhost:3000/main.html --output=html --output-path=./reports/lighthouse-report.html",
    "security": "npm audit && snyk test"
  },
  "keywords": [
    "whatsapp-bot",
    "bot-repair",
    "dead-bot-fix",
    "bot-development",
    "javascript",
    "web-development",
    "interactive-website",
    "mr-ntando",
    "bot-expert",
    "custom-bots",
    "voice-cloning",
    "ai-art",
    "bot-services"
  ],
  "author": {
    "name": "MR NTANDO OFC",
    "email": "contact@mrntando.com",
    "url": "https://github.com/mrntando"
  },
  "license": "MIT",
  "homepage": "https://mrntando.github.io/mr-ntando-ofc",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrntando/mr-ntando-ofc.git"
  },
  "bugs": {
    "url": "https://github.com/mrntando/mr-ntando-ofc/issues",
    "email": "support@mrntando.com"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^7.1.0",
    "gh-pages": "^6.0.0",
    "html-minifier-terser": "^7.2.0",
    "html-validate": "^8.7.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.3.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "snyk": "^1.1244.0",
    "terser": "^5.24.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "gsap": "^3.12.2",
    "particles.js": "^2.0.0",
    "typed.js": "^2.1.0"
  },
  "optionalDependencies": {
    "sharp": "^0.32.6"
  },
  "peerDependencies": {
    "fontawesome": "^6.0.0"
  },
  "config": {
    "port": 3000,
    "host": "localhost"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "js/**/*.js",
      "!js/vendor/**",
      "!**/node_modules/**"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/mrntando"
  },
  "contributors": [
    {
      "name": "MR NTANDO OFC",
      "email": "contact@mrntando.com",
      "role": "Lead Developer & Bot Expert"
    }
  ],
  "directories": {
    "lib": "./js",
    "test": "./tests",
    "doc": "./docs"
  },
  "files": [
    "js/",
    "css/",
    "images/",
    "*.html",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
